    "authors": [
      {
        "name": "LookAsLukas",
        "email": "kolyabetmen231006@gmail.com",
        "url": "https://github.com/LookAsLukas"
      }
    ],
    "links": {
      "repository": "https://github.com/LookAsLukas/tree-sitter-fap",
      "funding": "https://github.com/LookAsLukas/tree-sitter-fap"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
